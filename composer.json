{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "google/apiclient": "^2.18",
        "halestar/dicms-blogger": ">=0.7.5",
        "halestar/laravel-drop-in-cms": ">=0.7.5",
        "hashids/hashids": "^5.0",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/reverb": "^1.0",
        "laravel/scout": "^10.14",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5",
        "league/flysystem-aws-s3-v3": "^3.28",
        "livewire/livewire": "^3.5",
        "prism-php/prism": "^0.85.0",
        "spatie/laravel-permission": "^6.9",
        "tinymce/tinymce": "^7.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "ryoluo/sail-ssl": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["app/Utilities/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "halestar\\DiCmsBlogger\\": "packages/halestar/DiCmsBlogger/src",
            "halestar\\LaravelDropInCms\\": "packages/halestar/LaravelDropInCms/src",
            "halestar\\FabLmsGoogleIntegrator\\": "packages/halestar/FabLmsGoogleIntegrator/src"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "post-install-cmd": [
            "chmod -R 755 bootstrap\/cache",
            "php artisan optimize:clear",
            "php artisan optimize"
        ],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
          "Drive"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
